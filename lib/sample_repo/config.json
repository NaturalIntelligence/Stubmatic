{
  "datatables": "datatables/",
  "mappings": {
    "loadFromRecording": true,
    "default": {
      "request": {
        "method": "GET"
      },
      "response": {
        "strategy": "first-found",
        "latency": 0,
        "status": 200
      }
    },
    "files": ["mappings/response.yaml"]
  },
  "server": {
    "port": 7777
  },
  "stubs": "stubs/",
  "logs": {
    "path": "logs/"
  },
  "dumps": "dumps/",
  "recordAndPlay" : {
    "path" : "recordings",
    "mapping": {
      "headers" : true,
      "queryParam" : true,
      "hashStrig" : true,
      "skipHeaders" : []
    }
  }
}